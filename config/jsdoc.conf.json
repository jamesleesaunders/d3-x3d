{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"source": {
		"include": ["src"]
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"destination": "docs",
		"recurse": true,
		"readme": "README.md"
	}
}
